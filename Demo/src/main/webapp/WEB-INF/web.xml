<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee; http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>Demo</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.xhtml</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.xhtml</welcome-file>
	</welcome-file-list>
	
	<servlet>
	<servlet-name>abc</servlet-name>
	<servlet-class>com.tutedude.MultiplyServlet</servlet-class>
	<init-param>
	<param-name>name</param-name>
	<param-value>tutedude</param-value>
	</init-param>
	
	</servlet>
	
	<servlet-mapping>
	<servlet-name>abc</servlet-name>
	<url-pattern>/multiply</url-pattern>
	</servlet-mapping>
	
	<context-param>
	<param-name>name</param-name>
	<param-value>Harsh</param-value>
	</context-param>
	
	<filter>
	<filter-name>filter1</filter-name>
	<filter-class>com.tutedude.MyFilter</filter-class>
	
	<init-param>
	<param-name>name</param-name>
	<param-value>tutedude</param-value>
	</init-param>
	
	</filter>
	
	<filter-mapping>
	<filter-name>filter1</filter-name>
	<url-pattern>/multiply</url-pattern>
	</filter-mapping>
	
	<servlet>
	<servlet-name>abcd</servlet-name>
	<servlet-class>com.tutedude.CubedServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>abcd</servlet-name>
	<url-pattern>/cube</url-pattern>
	</servlet-mapping>
</web-app>
